<Window x:Class="Kolekcje1.Obliczanie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kolekcje1"
        mc:Ignorable="d"
        Title="Obliczanie" Height="300" Width="300">
    <Grid>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Center" Height="210" Margin="0,25,0,0" VerticalAlignment="Center" Width="250">
            <TextBox x:Name="textBox" VerticalScrollBarVisibility="Auto" IsReadOnly="True" />
        </Border>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Center" Margin="0,15,0,0" TextWrapping="Wrap" Text="Trwa obliczanie..." VerticalAlignment="Top"/>
    </Grid>
</Window>
