<Window x:Class="GUI4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI4"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="300">
    <Grid>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Center" Margin="0,0,0,50" TextWrapping="Wrap" Text="Podaj PESEL:" VerticalAlignment="Center"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Center" Height="23" Margin="0,0,0,0" VerticalAlignment="Center" Width="120"/>
        <Button x:Name="button" Content="Sprawdź poprawność" HorizontalAlignment="Center" Margin="0,0,0,-50" VerticalAlignment="Center" Width="120" Click="button_Click"/>
        <TextBlock x:Name="textBlock2" HorizontalAlignment="Center" Margin="0,0,0,-100" TextWrapping="Wrap"  VerticalAlignment="Center"/>
        <Button x:Name="button1" Content="Otwórz plik" HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="80" Click="button1_Click"/>
    </Grid>
</Window>
